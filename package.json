{
  "name": "chapatuchamba",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "webpack --mode production --entry ./functions/src/index.ts --output-path ./dist --devtool=source-map && source-map-explorer ./dist/main.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && tsc --noEmit",
    "build:web": "vite build",
    "build:functions": "cd functions && npm install --no-audit --no-fund && npm run build",
    "serve:functions": "cd functions && npm run serve",
    "deploy:functions": "npm run build:functions && firebase deploy --only functions",
    "deploy:prod": "npm run deploy:functions",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "firebase": "^12.6.0",
    "lucide-react": "^0.559.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
